	test -z "$(customdir)" || $(MKDIR_P) "$(DESTDIR)$(customdir)"
	@list='$(custom_DATA)'; test -n "$(customdir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(customdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(customdir)" || exit $$?; \
	done

