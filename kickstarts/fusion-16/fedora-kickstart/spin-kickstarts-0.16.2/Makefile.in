	@$(NORMAL_UNINSTALL)
	@list='$(custom_DATA)'; test -n "$(customdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
	echo " ( cd '$(DESTDIR)$(customdir)' && rm -f" $$files ")"; \
	cd "$(DESTDIR)$(customdir)" && rm -f $$files
